{
  "name": "@ips/proto-node",
  "author": "Fraunhofer FIT",
  "private": false,
  "version": "1.0.0",
  "license": "Apache-2.0",
  "types": "index.d.ts",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:js": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./ --grpc_out=. --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin --proto_path ../proto ../proto/*.proto",
    "build:ts": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=. -I ../proto ../proto/*.proto",
    "postinstall": "npm run build:js && npm run build:ts"
  },
  "devDependencies": {
    "grpc-tools": "^1.6.6",
    "grpc_tools_node_protoc_ts": "^2.4.2"
  }
}
